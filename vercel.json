{
  "version": 2,
  "builds": [
      {
          "src": "./index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/",
          "methods": [
            "GET",
            "HEAD",
            "PATCH",
            "POST",
            "DELETE",
            "OPTIONS"
          ],
          "headers": {
            "Access-Control-Allow-Origin": "https://trip-travel-ec0b4.web.app/"
          }
      }
  ]
}